  "common_map_patterns": {
    "title_only": {
      "description": "Map pour récupérer uniquement le titre",
      "properties_to_fetch": [1],
      "map_name_suffix": "Map"
    },
    "title_thumbnail": {
      "description": "Map pour titre + thumbnail",
      "properties_to_fetch": [1, "media"],
      "map_name_suffix": "Map",
      "additional_maps": ["ThumbnailMap"]
    },
    "title_thumbnail_url": {
      "description": "Map pour titre + thumbnail + URL",
      "properties_to_fetch": [1, 121, 174, 1517, "media"],
      "map_name_suffix": "Map",
      "additional_maps": ["ThumbnailMap", "PageMap"]
    }
